<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="nieuws")
 */
class Nieuws
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="date")
     */
    protected $datumtijd;


    /**
     * @ORM\Column(type="integer")
     */
    protected $jaar;

    /**
     * @ORM\Column(length=156)
     */
    protected $titel;

    /**
     * @ORM\Column(type="text")
     */
    protected $bericht;

}
