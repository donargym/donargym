<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="vakanties")
 */
class Vakanties
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(length=300)
     */
    protected $naam;

    /**
     * @ORM\Column(type="date")
     */
    protected $van;

    /**
     * @ORM\Column(type="date")
     */
    protected $tot;
}
