{% extends 'wedstrijdturnen/base.html.twig' %}

{% block inlogNavigation %}
    <div class="inlognavigation">
        <a href="{{ path('getWedstrijdturnenPage', { 'page': activeGroep.id }) }}">{{ activeGroep.name }}</a>
        > <b><i>Wedstrijduitslagen</i></b>
    </div>
{% endblock %}

{% block body %}
    <h1>{{ activeGroep.name }}</h1>
    <h2>Wedstrijduitslagen</h2>
    {% for key, value in wedstrijduitslagen %}
        <p><b>{{ key }}-{{ key + 1 }}</b><br>
        {% for wedstrijd in value %}
            <a href="/uploads/wedstrijduitslagen/{{ wedstrijd.locatie }}" target="blank">{{ wedstrijd.naam }}</a><br>
        {% endfor %}
        </p>
    {% endfor %}

{% endblock %}
